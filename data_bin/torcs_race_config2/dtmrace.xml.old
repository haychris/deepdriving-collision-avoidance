<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE params SYSTEM "params.dtd">


<params name="Challenge Race">
  <section name="Header">
    <attstr name="name" val="Challenge Race"/>
    <attstr name="description" val="Multiple races"/>
    <attnum name="priority" val="50"/>
    <attstr name="menu image" val="data/img/splash-dtm.png"/>
    <attstr name="run image" val="data/img/splash-run-practice.png"/>
    <attstr name="start image" val="data/img/splash-dtmstart.png"/>
  </section>

  <section name="Tracks">
    <attnum name="maximum number" val="1"/>
    <section name="1">
      <attstr name="name" val="chenyi-e-track-6"/>
      <attstr name="category" val="road"/>
    </section>

  </section>

  <section name="Races">
    <section name="1">
      <attstr name="name" val="Sprint Race"/>
    </section>

  </section>


  <section name="Sprint Race">
    <attnum name="distance" unit="km" val="1"/>
    <attstr name="type" val="race"/>
    <attstr name="starting order" val="display list"/>
    <attstr name="restart" val="yes"/>
    <attnum name="maximum drivers" val="10"/>
    <attstr name="splash menu" val="no"/>
    <section name="Starting Grid">
      <attnum name="rows" val="1"/>
      <attnum name="distance to start" val="0"/>
      <attnum name="distance between columns" val="140"/>
      <attnum name="offset within a column" val="10"/>
      <attnum name="initial speed" val="70"/>
      <attnum name="initial height" val="0.2"/>
    </section>

    <section name="Points">
      <section name="1">
        <attnum name="points" val="10"/>
      </section>

      <section name="2">
        <attnum name="points" val="8"/>
      </section>

      <section name="3">
        <attnum name="points" val="6"/>
      </section>

      <section name="4">
        <attnum name="points" val="5"/>
      </section>

      <section name="5">
        <attnum name="points" val="4"/>
      </section>

      <section name="6">
        <attnum name="points" val="3"/>
      </section>

      <section name="7">
        <attnum name="points" val="2"/>
      </section>

      <section name="8">
        <attnum name="points" val="1"/>
      </section>

    </section>

  </section>

  <section name="Drivers">
    <attnum name="maximum number" val="10"/>
    <attstr name="focused module" val="chenyi"/>
    <attnum name="focused idx" val="1"/>
    <section name="1">
      <attnum name="idx" val="1"/>
      <attstr name="module" val="chenyi_AI2"/>
    </section>

    <section name="2">
      <attnum name="idx" val="1"/>
      <attstr name="module" val="chenyi"/>
    </section>

  </section>

  <section name="Configuration">
    <attnum name="current configuration" val="3"/>
    <section name="1">
      <attstr name="type" val="track select"/>
    </section>

    <section name="2">
      <attstr name="type" val="drivers select"/>
    </section>

  </section>

</params>
